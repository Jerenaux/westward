{
  "name": "westward",
  "version": "1.0.0",
  "description": "Westward, a collaborative MMO",
  "private": true,
  "main": "./dist/server.js",
  "directories": {
    "test": "test"
  },
  "nodemonConfig": {
    "watch": [
      "dist/server.js"
    ],
    "ext": "js",
    "ignore": [
      "maps",
      "client",
      "server",
      "admin",
      "assets",
      "node_modules"
    ]
  },
  "scripts": {
    "browser-sync": "browser-sync start --config bs-config.js",
    "build": "webpack",
    "dev": "concurrently \"npm run watch:server\" \"npm run watch:client\"",
    "test": "mocha ./dist/test.js",
    "watch:server": "nodemon ./dist/server.js",
    "watch": "webpack-dev-server"
  },
  "engines": {
    "npm": "^5.7.1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.2.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "dir-compare": "^1.7.3",
    "jimp": "^0.6.4",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "mongo-express": "^0.51.2",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "sinon": "^7.4.1",
    "spritesmith": "^3.4.0",
    "style-loader": "^1.0.0",
    "uglify-js": "^3.6.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1",
    "xml2js": "^0.4.22"
