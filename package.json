    "clone": "^2.1.2",
    "config": "^1.31.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "keycloak-connect": "3.3.0",
    "mongodb": "^2.2.36",
    "mongoose": "^5.7.3",
